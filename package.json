  "author": "S MUNI HARISH (smuniharish) <samamuniharish@gmail.com> (https://github.com/smuniharish/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/parser": "^7.22.7",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "lib/**/*"
