{
  "name": "@smh/jst-functions",
  "version": "0.0.1",
  "description": "Locale Helper",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint .",
